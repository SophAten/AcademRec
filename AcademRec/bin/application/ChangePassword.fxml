<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" spacing="20.0"
	xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.ChangePasswordController">
	<children>
		<Text text="Change Password">
			<font>
				<Font name="Noteworthy Bold" size="36.0" />
			</font>
		</Text>
		<TextField promptText="Old Password" />
		<TextField promptText="New Password" />
		<TextField promptText="Confirm New Password" />
		<Button text="Submit" onAction="#handleSubmitButtonClick" />
	</children>
	<padding>
		<Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
	</padding>
</VBox>
