<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0"
	prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.171"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.ResetPasswordController">
	<top>
		<AnchorPane prefHeight="100.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<children>
				<Label layoutX="114.0" layoutY="14.0" text="Welcome New User!">
					<font>
						<Font name="Bell MT" size="44.0" />
					</font>
				</Label>
				<Label layoutX="167.0" layoutY="66.0"
					text="Please Create A Password">
					<font>
						<Font name="Bell MT" size="24.0" />
					</font>
				</Label>
			</children>
		</AnchorPane>
	</top>
	<center>
		<AnchorPane prefHeight="200.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<children>
				<Label layoutX="132.0" layoutY="46.0" text="New Password" />
				<PasswordField fx:id="passwordField" layoutX="262.0"
					layoutY="41.0" onAction="#resetPassword" />
				<Button fx:id="changePass" layoutX="239.0" layoutY="197.0"
					mnemonicParsing="false" onAction="#resetPassword"
					text="Reset Password" />
				<PasswordField fx:id="checkPassword" layoutX="262.0"
					layoutY="107.0" onAction="#resetPassword" />
				<Label layoutX="120.0" layoutY="112.0" text="Confirm Password" />
				<Label fx:id="wrongPass" alignment="CENTER" layoutX="132.0"
					layoutY="161.0" prefHeight="23.0" prefWidth="336.0" textFill="RED">
					<font>
						<Font name="Bell MT" size="15.0" />
					</font>
				</Label>
			</children>
		</AnchorPane>
	</center>
</BorderPane>
